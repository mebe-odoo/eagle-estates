<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="eagle_property_view_form" model="ir.ui.view">
            <field name="name">eagle.property.form.view</field>
            <field name="model">eagle.property</field>
            <field name="arch" type="xml">
                <form string="Property">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="construction_date"/>
                            <field name="surface"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="eagle_property_view_tree" model="ir.ui.view">
            <field name="name">eagle.property.tree.view</field>
            <field name="model">eagle.property</field>
            <field name="arch" type="xml">
                <tree string="Properties">
                    <field name="name"/>
                    <field name="construction_date"/>
                    <field name="surface"/>
                </tree>
            </field>
        </record>

        <record id="eagle_properties_action" model="ir.actions.act_window">
            <field name="name">Properties</field>
            <field name="res_model">eagle.property</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="menu_eagle_estates_root" name="Eagle Estates" web_icon="eagle_estates,static/description/icon.png">
            <menuitem id="menu_eagle_estates_properties" action="eagle_properties_action" name="Properties" sequence="10"/>
        </menuitem>

    </data>
</odoo>